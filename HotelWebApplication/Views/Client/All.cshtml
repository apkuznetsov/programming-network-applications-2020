@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    @using (Html.BeginForm("Search", "Client", FormMethod.Post, new { @class = "navbar-form navbar-left" }))
    {
        <form class="navbar-nav pull-right">
            <input type="text" name="search" placeholder="Поиск..." class="search-field" />
        </form>
    }

    <h1>Список клиентов</h1>
    <table class="room-foreach">
        <tr>
            <td><p>Серия и номер паспорта</p></td>
            <td><p>ФИО</p></td>
        </tr>
        @foreach (var c in ViewBag.Clients)
        {
            <tr>
                <td><p>@c.PassportSeriesAndNumber</p></td>
                <td><p>@c.FullName</p></td>
            </tr>
        }
    </table>
</div>